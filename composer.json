{
    "name": "milenagrygier/money-package",
    "description": "money-squad tests",
    "type": "library",
    "authors": [
        {
            "name": "Milena Grygier",
            "email": "milenagrygier@protonmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Money\\MoneyPackage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Money\\MoneyPackage\\Tests\\": "tests",
            "Money\\MoneyPackage\\Database\\Factories\\": "database/factories"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Money\\MoneyPackage\\Providers\\MoneyServiceProvider"
            ]
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
